        android:name="com.example.skydelight.navbar.HomeFragment"
        android:label="fragment_principal"
        tools:layout="@layout/fragment_navbar_home" >
        <action
            android:id="@+id/action_navBarHome_to_navBarTest"
            app:destination="@id/navbar_test_fragment"
            app:enterAnim="@anim/slide_from_right"
            app:exitAnim="@anim/slide_exit_to_left"
            app:popEnterAnim="@anim/slide_from_left"
            app:popExitAnim="@anim/slide_exit_to_right"
            app:popUpTo="@+id/navbar_home_fragment"
            app:popUpToInclusive="true" />
